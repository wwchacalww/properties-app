// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Properties {
  id          String  @id @default(uuid())
  description String
  code        String
  room        String
  labeled     Boolean
  status      Boolean
  page        Int
  line        Int

  @@map("properties")
}
